

namespace MyScrabble.Controller.Tiles
{
    abstract class Tile
    {
        private readonly char letter;

        private readonly int points;

        private readonly string imageURI;

        protected Tile(char letter, int points, string imageURI)
        {
            this.letter = letter;
            this.points = points;
            this.imageURI = imageURI;

        }
    }
}
